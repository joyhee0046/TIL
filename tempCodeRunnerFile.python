T = int(input())

for tc in range(1, T+1) :
    N = int(input())
    li = [list(map(int, input())) for _ in range(N)]
    j = N//2
    k = N//2 +1
    ans = 0
    for i in range(N) :
        for ii in range(j,k) :
            ans = ans + li[i][ii]
        if j>=0 :
            j-=1
            k+=1
        else :
            j+=1
            k-=1
    print(ans)